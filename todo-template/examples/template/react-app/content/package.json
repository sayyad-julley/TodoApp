{
  "name": "{{ __APP_NAME__ }}",
  "description": "{{ description }}",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "{{ reactVersion }}",
    "react-dom": "{{ reactVersion }}"
    {{#if useRouter}}
    ,"react-router-dom": "^6.8.0"
    {{/if}}
    {{#if useStateManagement}}
      {{#if (eq useStateManagement 'zustand')}}
    ,"zustand": "^4.3.0"
      {{/if}}
      {{#if (eq useStateManagement 'redux-toolkit')}}
    ,"@reduxjs/toolkit": "^1.9.0","react-redux": "^8.0.0"
      {{/if}}
    {{/if}}
    {{#if (eq uiFramework 'ant-design')}}
    ,"antd": "^5.2.0"
    {{/if}}
    {{#if (eq uiFramework 'material-ui')}}
    ,"@mui/material": "^5.11.0","@emotion/react": "^11.10.0","@emotion/styled": "^11.10.0"
    {{/if}}
    {{#if (eq uiFramework 'chakra-ui')}}
    ,"@chakra-ui/react": "^2.5.0","@emotion/react": "^11.10.0","@emotion/styled": "^11.10.0","framer-motion": "^10.0.0"
    {{/if}}
  },
  "devDependencies": {
    "@types/react": "{{ reactVersion }}",
    "@types/react-dom": "{{ reactVersion }}",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^2.8.0",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "vitest": "^0.30.0",
    "@vitest/ui": "^0.30.0",
    "@vitest/coverage-v8": "^0.30.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-vite": "^7.0.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "prop-types": "^15.8.0",
    "storybook": "^7.0.0"
    {{#if (eq uiFramework 'tailwind')}}
    ,"tailwindcss": "^3.3.0","autoprefixer": "^10.4.0","postcss": "^8.4.0"
    {{/if}}
  }
}